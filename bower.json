{
  "name": "bootstrap-here-sass",
  "version": "3.3.1",
  "homepage": "https://github.com/oncast-labs/bootstrap-here-sass",
  "authors": [
    "Thomas McDonald",
    "Tristan Harward",
    "Peter Gumeson",
    "Gleb Mazovetskiy",
    "Pablo Bender"
  ],
  "description": "bootstrap-sass is a Sass-powered version of Bootstrap, modified to affect only elements within outer element with attribute data-bootstrap-here.",
  "main": [
    "assets/stylesheets/_bootstrap.scss",
    "assets/fonts/bootstrap/glyphicons-halflings-regular.eot",
    "assets/fonts/bootstrap/glyphicons-halflings-regular.svg",
    "assets/fonts/bootstrap/glyphicons-halflings-regular.ttf",
    "assets/fonts/bootstrap/glyphicons-halflings-regular.woff",
    "assets/javascripts/bootstrap/affix.js",
    "assets/javascripts/bootstrap/alert.js",
    "assets/javascripts/bootstrap/button.js",
    "assets/javascripts/bootstrap/carousel.js",
    "assets/javascripts/bootstrap/collapse.js",
    "assets/javascripts/bootstrap/dropdown.js",
    "assets/javascripts/bootstrap/tab.js",
    "assets/javascripts/bootstrap/transition.js",
    "assets/javascripts/bootstrap/scrollspy.js",
    "assets/javascripts/bootstrap/modal.js",
    "assets/javascripts/bootstrap/tooltip.js",
    "assets/javascripts/bootstrap/popover.js"
  ],
  "keywords": [
    "bootstrap",
    "bootstrap-here"
    "sass"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "lib",
    "tasks",
    "templates",
    "test",
    "*.gemspec",
    "Rakefile",
    "Gemfile"
  ],
  "dependencies": {
    "jquery": ">= 1.9.0"
  }
}
